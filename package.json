{
  "name": "@uiw/react-mac-keyboard",
  "version": "1.0.5",
  "description": "Macbook computer keyboard style for react component.",
  "homepage": "https://uiwjs.github.io/react-mac-keyboard/",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "scripts": {
    "prepack": "npm run build",
    "doc": "ENTRYDIR=website kkt build",
    "doc:dev": "ENTRYDIR=website kkt start",
    "build": "npm run build:lib && npm run doc",
    "watch": "npm run types:watch & npm run ts:watch",
    "build:lib": "npm run ts:build && npm run types:esm && npm run types:cjs && npm run css:build",
    "types:build": "tsbb types --sourceRoot src --target ESNEXT",
    "types:watch": "npm run types:esm -- --watch & npm run types:cjs -- --watch",
    "types:esm": "npm run types:build -- --outDir ../lib/esm",
    "types:cjs": "npm run types:build -- --outDir ../lib/cjs",
    "css:build": "compile-less -d src -o lib/esm",
    "ts:watch": "tsbb watch --env-name esm:dev --env-name cjs --target react",
    "ts:build": "tsbb build --target react"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/uiwjs/react-mac-keyboard.git"
  },
  "keywords": [
    "mac",
    "uiw",
    "react",
    "react-component",
    "mac-keyboard",
    "macbook",
    "keyboard"
  ],
  "author": "kenny wong <wowohoo@qq.com>",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "dependencies": {
    "prop-types": "15.7.2"
  },
  "devDependencies": {
    "@kkt/loader-less": "5.8.0",
    "@kkt/loader-raw": "5.8.0",
    "@types/classnames": "2.2.10",
    "@types/react": "16.9.34",
    "@types/react-dom": "16.9.7",
    "@uiw/react-github-corners": "1.1.3",
    "@uiw/react-markdown-preview": "1.0.5",
    "@uiw/react-shields": "1.1.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "compile-less-cli": "1.4.0",
    "kkt": "5.8.0",
    "tsbb": "1.7.6"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
